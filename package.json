{
  "name": "trade-io",
  "version": "0.1.0",
  "description": "Production-grade paper trading platform for learning and competition",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "node .output/server/index.mjs",
    "preview": "nuxt preview",
    "postinstall": "prisma generate",
    "test": "vitest run",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "test:e2e": "playwright test",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,vue,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,vue,js,json,md}\"",
    "typecheck": "vue-tsc --noEmit && tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:create": "prisma migrate dev --create-only",
    "db:migrate:reset": "prisma migrate reset",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "prepare": "husky || true"
  },
  "keywords": [
    "trading",
    "paper-trading",
    "stocks",
    "portfolio",
    "finance",
    "investment",
    "education"
  ],
  "author": "Logan Renz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/loganrenz/trade-io.git"
  },
  "bugs": {
    "url": "https://github.com/loganrenz/trade-io/issues"
  },
  "homepage": "https://github.com/loganrenz/trade-io#readme",
  "dependencies": {
    "@pinia/nuxt": "^0.11.3",
    "@prisma/client": "^5.22.0",
    "@supabase/supabase-js": "^2.47.10",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@vueuse/core": "^11.3.0",
    "@vueuse/nuxt": "^11.3.0",
    "decimal.js": "^10.6.0",
    "nuxt": "^3.15.2",
    "pinia": "^3.0.4",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "superjson": "^2.2.2",
    "trpc-nuxt": "^0.10.22",
    "vee-validate": "^4.14.7",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.1",
    "@iconify-json/lucide": "^1.2.16",
    "@nuxt/devtools": "^1.6.4",
    "@nuxt/eslint-config": "^0.7.3",
    "@nuxt/test-utils": "^3.15.2",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@playwright/test": "^1.49.1",
    "@types/node": "^20.19.26",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.31.0",
    "happy-dom": "^15.11.7",
    "husky": "^9.1.7",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vite": "^6.0.7",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.0"
  }
}
